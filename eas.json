{
  "cli": {
    "version": ">= 16.7.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "app-bundle",
        "env": {
          "NPM_CONFIG_LEGACY_PEER_DEPS": "true",
          "NPM_CONFIG_FORCE": "true"
        },
        "cache": {
          "disabled": true
        }
      }
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}